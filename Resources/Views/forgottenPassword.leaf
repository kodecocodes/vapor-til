#set("content") {
  <h1>#(title)</h1>

  <form method="post">
    <div class="form-group">
      <label for="email">Email</label>
      <input type="email" name="email" class="form-control"
       id="email"/>
    </div>

    <button type="submit" class="btn btn-primary">
      Reset Password
    </button>
  </form>
}

#embed("base")
